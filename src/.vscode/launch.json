            "request": "launch",
            "program": "${workspaceFolder}/tsp_generator.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["100","../data/tsp_instances/","1","200","progen"]
        },
        {
            "name": "Train Agent",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train_agent.py",
            "console": "integratedTerminal",
